%!TEX root = ../Main.tex
\newcommand\WrapMode[3]
{                   #1 ~\xRightarrow{#2}~ #3
}

\newcommand\WrapApp[3]
{                   #1~\cdot~#2 ~\xRightarrow~ #3
}


\newcommand\Typecheck[3]
{                   #1
        ~\vdash~    #2 ~:~ #3
}


\newcommand\TypecheckPrim[2]
{       #1 ~:_{P}~ #2
}

\newcommand\Bigstep[5]
{       (#1) ~\cdot~ (#2) ~|~ #3 ~\Downarrow~ #4 ~\cdot~ #5
}
\newcommand\BigstepG[3]
{       \ov{\Gamma} ~\cdot~ \Gamma ~|~ #1 ~\Downarrow~ #2 ~\cdot~ #3
}

\newcommand\BigstepInsert[3]
{ \mit{insert}~#1~#2~#3
}

\newcommand\ToCore[4]
{       #1 ~\Rightarrow_{\mathtt{Core}}~ #2\mathtt{;}~#3\mathtt{;}~#4
}


\newcommand\NN  {\mathbb{N}}
\newcommand\BB  {\mathbb{B}}

\newcommand\Elm[1] {\mathtt{Element}~#1}
\newcommand\Agg[1] {\mathtt{Aggregate}~#1}
